
{
"version": 2,
"builds": [
           {"src": "django_quant/wsgi.py",
             "use": "@vercel/python",
             "config": { "maxLambdaSize": "15mb", "runtime": "@vercel/python" }
           }
          ],

"routes": [
           {"src": "/(.*)",
            "dest": "django_quant/wsgi.py"
            }
          ] 

}
